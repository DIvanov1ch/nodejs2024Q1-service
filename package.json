    "@types/express": "^4.17.21",
    "@types/jest": "27.4.1",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.18.87",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^8.3.4",
